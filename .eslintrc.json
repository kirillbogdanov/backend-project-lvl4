{
    "env": {
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "plugins": [
        "jest"
    ],
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "ignorePatterns": ["assets"],
    "rules": {
        "import/extensions": 0
    }
}
